<Window x:Class="BingMapsWPF.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BingMapsWPF"
        mc:Ignorable="d"
        Title="Window4" Height="200" Width="300">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="42,30,0,0" TextWrapping="Wrap" Text="Please enter name of new Folder below..." VerticalAlignment="Top" Height="46" Width="203"/>
        <TextBox x:Name="nameBox" HorizontalAlignment="Left" Margin="42,85,0,0" TextWrapping="Wrap" Text="enter name here" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="enterButton" Content="Enter" HorizontalAlignment="Left" Margin="167,85,0,0" VerticalAlignment="Top" Width="48" Click="enterButton_Click"/>
    </Grid>
</Window>
